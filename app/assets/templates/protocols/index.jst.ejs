<h1>Listing protocols</h1>

<table id="protocols-list">
  <thead>
    <tr>
      <th>Name</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>
  <tbody>

<% protocols.each(function(model) { %>
  <tr id="protocol_<%= model.id %>">
    <td><%= model.escape('name') %></td>
    <td><a href="protocols/<%= model.id %>">Show</a></td>
    <td><a href="protocols/<%= model.id %>/edit">Edit</a></td>
    <td><a href="protocols/<%= model.id %>" data-confirm="Are you sure?" data-method="delete" rel="nofollow">Destroy</a></td>
  </tr>
<% }); %>

  </tbody>
</table>

<br>

<a class="create" href="protocols/new">New Protocol</a>
